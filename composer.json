{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-zip": "*",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "fideloper/proxy": "^4.4.0",
        "guzzlehttp/guzzle": "^6.5.5",
        "intervention/image": "^2.5.1",
        "kevinrob/guzzle-cache-middleware": "^3.3.1",
        "laravel/framework": "^5.8.38",
        "laravel/tinker": "^1.0.10",
        "silber/bouncer": "v1.0.0-rc.5",
        "symfony/translation": "4.3.8",
        "tymon/jwt-auth": "1.0.0-rc.4.1",
        "yajra/laravel-datatables-oracle": "^9.10.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.3.0",
        "filp/whoops": "^2.7.2",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^3.0.1",
        "phpunit/phpunit": "^7.5.20"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
